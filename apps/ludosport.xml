            visible="false" attribution="Map tiles by  &lt;a href='http://cartodb.com/attributions'>CartoDb &lt;/a>, under  &lt;a href='http://creativecommons.org/licenses/by/3.0/'>CC BY 3.0 &lt;/a>" />
        <baselayer  type="OSM" id="positron" label="Positron" title="CartoDb" thumbgallery="img/basemap/positron.png"
            url="http://{a-c}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png" maxzoom="20"
            visible="true" attribution="Map tiles by  &lt;a href='http://cartodb.com/attributions'>CartoDb &lt;/a>, under  &lt;a href='http://creativecommons.org/licenses/by/3.0/'>CC BY 3.0 &lt;/a>" />
        <baselayer  type="OSM" id="osm1" label="OpenStreetMap" title="OpenStreetMap" thumbgallery="img/basemap/osm.png"
            url="http://{a-c}.tile.openstreetmap.org/{z}/{x}/{y}.png"
            attribution="© MapQuest. Données : les contributeurs d'&lt;a href='http://www.openstreetmap.org/' target='_blank'>OpenStreetMap &lt;/a>,  &lt;a href='http://www.openstreetmap.org/copyright' target='_blank'>ODbL &lt;/a>" visible="false"/>
    </baselayers>
    <proxy url=""/>
    <olscompletion url="http://api-adresse.data.gouv.fr/search/" type="ban"/>
    <searchparameters localities="true" features="true" bbox="false"/>

    <themes mini="false">
      <theme name="Activités ludiques"  collapsed="true" id="photos" icon="fas fa-basketball-ball">
        <layer id="actnatres" name="Activités ludiques"  visible="true"  queryable="true"
                    fields="equnm,actlb,eqptl,source,grdeqptl"
                    aliases="Nom de l'équipement,Libellé de l'activité,Libellé du type d'équipement,Source,Catégorie du type d'équipement"
                    infoformat="application/vnd.ogc.gml" featurecount="20" infopanel="right-panel"
                    url="http://193.48.28.84:8080/geoserver/applications/wms?"
                    attribution="IGN"
                    searchable="true"
                    searchengine="fuse"
                    fusesearchkeys="equnm"
                    fusesearchresult="{{equnm}}">
            <template url="apps/ludosport/actnatres.mst"></template>
        </layer>
      </theme>
    	<theme name="Parcs marins"  collapsed="true" id="parc" icon="fas fa-fish">
    		<layer id="sites_etude_rep_amp" name="Zones d'étude"  visible="true"  queryable="true"
                    fields="nom,id,effectif_etatetabpu,effectif_collter,effectif_assosenv,effectif_usalois,effectif_persqual,effectif_orgreppro,effectif_autresespa,total,nom_complet"
                    aliases="Nom,ID,Etat,Collectivité Territoriale,Associations environementales,Usagers,Personnes qualifiées,Organisations pro,Autres,Total,Nom"
                    infoformat="application/vnd.ogc.gml" type="customlayer" vectorlegend="true" featurecount="20" infopanel="right-panel"
	                  url="apps/ludosport/customlayers/sites_etude_rep_amp.js"
                    customcontrol="true"
                    customcontrolpath="apps/ludosport/customcontrols/"
	                  attribution="UMR LISA"
                    expanded="true"
                    searchable="true"
                    searchengine="fuse"
                    fusesearchkeys="nom_complet"
                    fusesearchresult="{{nom_complet}}">
		            <template url="apps/ludosport/sites_etude_rep_amp.mst"></template>
		        </layer>
		</theme>
